{
  "$schema": "https://schemas.wp.org/wp/6.6/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "position": {
      "sticky": true
    },
    "dimensions": {
      "minHeight": false
    },
    "useRootPaddingAwareAlignments": true,
    "layout": {
      "wideSize": "calc(100vw - 2 * var(--wide-constrainer, 20px))",
      "contentSize": "calc(100vw - 2 * var(--constrainer, 20px))"
    },
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true,
      "defaultSpacingSizes": false,
      "spacingSizes": [
        {
          "size": "5px",
          "slug": "5",
          "name": "5px"
        },
        {
          "size": "10px",
          "slug": "10",
          "name": "10px"
        },
        {
          "size": "20px",
          "slug": "20",
          "name": "20px"
        },
        {
          "size": "40px",
          "slug": "40",
          "name": "40px"
        },
        {
          "size": "80px",
          "slug": "80",
          "name": "80px"
        },
        {
          "size": "160px",
          "slug": "160",
          "name": "160px"
        }
      ]
    },
    "color": {
      "custom": true,
      "duotone": [],
      "customDuotone": false,
      "defaultDuotone": false,
      "gradients": [],
      "customGradient": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        {
          "name": "Gray",
          "slug": "gray",
          "color": "#eee"
        },
        {
          "name": "White",
          "slug": "white",
          "color": "#fff"
        },
        {
          "name": "Black",
          "slug": "black",
          "color": "#000"
        }
      ]
    },
    "typography": {
      "customFontSize": true,
      "dropCap": false,
      "letterSpacing": true,
      "lineHeight": true,
      "textDecoration": true,
      "textTransform": true,
      "defaultFontSizes": false,
      "fluid": {
        "minFontSize": "16px",
        "maxViewportWidth": "1600px",
        "minViewportWidth": "480px"
      },
      "fontFamilies": [
        {
          "name": "Heading",
          "slug": "heading",
          "fontFamily": "Montserrat, system-ui, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif"
        },
        {
          "name": "Body",
          "slug": "body",
          "fontFamily": "Open Sans, system-ui, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif"
        }
      ],
      "fontSizes": [
        {
          "slug": "type-h1",
          "size": "50px",
          "name": "H1",
          "fluid": {
            "min": "40px"
          }
        },
        {
          "slug": "type-h2",
          "size": "40px",
          "name": "H2",
          "fluid": {
            "min": "35px"
          }
        },
        {
          "slug": "type-h3",
          "size": "35px",
          "name": "H3",
          "fluid": {
            "min": "30px"
          }
        },
        {
          "slug": "type-h4",
          "size": "30px",
          "name": "H4",
          "fluid": {
            "min": "25px"
          }
        },
        {
          "slug": "type-h5",
          "size": "25px",
          "name": "H5",
          "fluid": {
            "min": "20px"
          }
        },
        {
          "slug": "type-h6",
          "size": "20px",
          "name": "H6",
          "fluid": {
            "min": "18px"
          }
        },
        {
          "slug": "type-body",
          "size": "18px",
          "name": "Body",
          "fluid": {
            "min": "16px"
          }
        },
        {
          "slug": "type-body-small",
          "size": "14px",
          "name": "Body - Small"
        },
        {
          "slug": "type-body-large",
          "size": "24px",
          "name": "Body - Large",
          "fluid": {
            "min": "20px"
          }
        }
      ]
    },
    "custom": {
      "myCustomVariables": {
        "primary": "#000",
        "secondary": "#fff"
      }
    }
  },
  "styles": {
    "elements": {
      "link": {
        "typography": {
          "textDecoration": "none"
        }
      }
    },
    "spacing": {
      "blockGap": "3rem",
      "padding": {
        "top": "0",
        "bottom": "0",
        "left": "1.25rem",
        "right": "1.25rem"
      }
    }
  },
  "templateParts": [
    {
      "name": "logo",
      "title": "Site Logo",
      "area": "uncategorized"
    },
    {
      "name": "archive",
      "title": "Template: Archive",
      "area": "uncategorized"
    },
    {
      "name": "search",
      "title": "Template: Search",
      "area": "uncategorized"
    },
    {
      "name": "single-post",
      "title": "Template: Single Post",
      "area": "uncategorized"
    },
    {
      "name": "404",
      "title": "Template: 404",
      "area": "uncategorized"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "uncategorized"
    }
  ]
}
