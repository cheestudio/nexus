{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"version": "1",
	"name": "mac/tabs",
	"title": "mac Tabs",
	"category": "custom-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"tabStyle": {
			"type": "string",
			"default": "normal",
			"selector": ".mac-tabs"
		},
		"backgroundColor": {
			"type": "string",
			"default": "",
			"selector": ".mac-tabs"
		},
		"textColor": {
			"type": "string",
			"default": "",
			"selector": ".mac-tabs"
		},
		"activeTabIndex": {
			"type": "number",
			"default": 0,
			"selector": ".mac-tabs"
		}
	}
} 